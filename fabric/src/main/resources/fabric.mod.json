{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${display_name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "issues": "${issues}",
    "sources": "${source}"
  },
  "license": "${license}",
  "icon": "${icon}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.mrjulsen.blockbeats.fabric.BlockBeatsFabric"
    ]
  },
  "mixins": [
    "${modid}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java_version}",
    "dragonlib": ">=${dragonlib_version}",
    "dragnsounds": ">=${dragnsounds_version}",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/AeSbNgvc7f",
        "DragonLib 2": "https://github.com/MisterJulsen/MC-DragonLib2",
        "DragNSounds API": "https://misterjulsen.github.io/DragNSounds-API/",
        "JAVE2": "https://github.com/a-schild/jave2"
      },
      "update_checker": true
    }
  },
  "accessWidener": "${modid}.accesswidener"
}
